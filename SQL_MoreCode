  (1007, 'Kevin', 'Malone', 18, 'Male')
  (1008, 'Angela', 'Davis', 32, 'Female')
  (1009, 'Katy', 'Stone', 38, 'Female')

CREATE TABLE EmployeeSalary
(
EmployeeID int,
JobTitle Varchar(50),
Salary int
)
Insert Into EmployeeSalary VALUES
(1001, 'Salesman', 45000),
(1002, 'Receptionist', 36000),
(1003, 'Salesman', 63000),
(1004, 'Accountant', 47000),
(1005, 'HR', 50000),
(1006, 'Regional Manager', 65000),
(1007, 'Supplier Relations', 41000),
(1008, 'Salesman', 48000),
