SELECT FirstName, LastName FROM EmployeeDemographics

SELECT DISTINCT(Age)
FROM EmployeeDemographics

SELECT COUNT(lastname) AS LastNameCount
FROM EmployeeDemographics

SELECT * FROM EmployeeSalary

SELECT MAX(salary) 
FROM EmployeeSalary

SELECT MAX(salary) 
FROM EmployeeSalary

SELECT * 
From EmployeeDemographics
WHERE firstname = 'Katy'

SELECT * 
From EmployeeDemographics
WHERE firstname <> 'Katy'

SELECT * 
From EmployeeDemographics
WHERE age >= 40 AND Gender ='Male'

SELECT * 
From EmployeeDemographics
WHERE lastname LIKE 'S%'

SELECT * 
From EmployeeDemographics
WHERE lastname LIKE '%S%'

SELECT * 
From EmployeeDemographics
WHERE lastname LIKE 'S%o%'

SELECT * 
From EmployeeDemographics
WHERE lastname IN('Davis', 'Bowen')

SELECT gender, COUNT(gender)
From EmployeeDemographics
GROUP BY gender, age

SELECT gender, age, COUNT(gender)
From EmployeeDemographics
WHERE age >= 40
GROUP BY gender, age

SELECT *
From EmployeeDemographics
ORDER BY age ASC
SELECT *
From EmployeeDemographics
ORDER BY age 
